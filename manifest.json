{
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "https://t2-herriot.vetrf.ru/*",
    "https://herriot.vetrf.ru/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}